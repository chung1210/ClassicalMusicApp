
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}


    <!-- Single Music Charts Details Area Start -->
    <section class="single-music-charts-details bg-overlay bg-img section-padding-80-0 jarallax" style="background-image: url(/Asset/img/bg-img/34.jpg);">

        <div class="container">
            <div class="row align-items-center">
                <!-- Thumbnail -->
                <div class="col-12 col-md-6">
                    <div class="chart-details-thumbnail mb-80">
                        <img src="~/Asset/img/bg-img/album.png" alt="">
                    </div>
                </div>
                <!-- Content -->
                <div class="col-12 col-md-6">
                    <div class="chart-details-content mb-80">
                        <h2>Day Dream</h2>
                        <p>Deam Ft J-Snopy</p>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Single Music Charts Details Area End -->
    <!-- Music Charts Area Start -->
    <section class="razo-music-charts-area section-padding-80">
        <div class="container">
            <div class="row">
                <div class="col-12">

                    <!-- Single Music Chart -->
                    <div class="single-music-chart style-2 d-flex align-items-center justify-content-between wow fadeInUp" data-wow-delay="100ms">
                        <!-- Music Content -->
                        <div class="music-content d-flex align-items-center">
                          
                            <div class="music-thumb">
                                <img src="~/Asset/img/bg-img/25.jpg" alt="">
                            </div>
                            <div class="audio-player">
                                <audio preload="auto" controls>
                                    <source src="~/Asset/audio/dummy-audio.mp3">
                                </audio>
                            </div>
                            <div class="music-title">
                                <h5>Way Back Home - <span>RadioDaily Show</span></h5>
                            </div>
                        </div>
                       
                    </div>


                </div>
            </div>
        </div>
    </section>
    <!-- Music Charts Area End -->
    <!-- You May Like Area Start -->
    <section class="razo-you-may-like-area">
        <div class="container">
            <div class="row">
                <!-- Section Heading -->
                <div class="col-sm-6">
                    <div class="section-heading">
                        <h2>YOU MAY ALSO LIKE</h2>
                    </div>
                </div>
                <!-- Show All Button -->
                <div class="col-sm-6">
                    <div class="show-all-button mb-50 text-right">
                        <a href="#" class="btn show-all-btn">Show More Charts</a>
                    </div>
                </div>
            </div>

            <div class="row">
                <!-- Single Charts Portfolio -->
                <div class="col-12 col-md-6 col-lg-4">
                    <div class="single-charts-portfolio mb-80">
                        <img src="~/Asset/img/bg-img/35.jpg" alt="">
                        <!-- Overlay -->
                        <div class="overlay-content">
                            <div class="text-center">
                                <h5>Screaming Females</h5>
                                <p>January 15, 2018</p>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Single Charts Portfolio -->
                <div class="col-12 col-md-6 col-lg-4">
                    <div class="single-charts-portfolio mb-80">
                        <img src="~/Asset/img/bg-img/36.jpg" alt="">
                        <!-- Overlay -->
                        <div class="overlay-content">
                            <div class="text-center">
                                <h5>Screaming Females</h5>
                                <p>January 15, 2018</p>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Single Charts Portfolio -->
                <div class="col-12 col-md-6 col-lg-4">
                    <div class="single-charts-portfolio mb-80">
                        <img src="~/Asset/img/bg-img/37.jpg" alt="">
                        <!-- Overlay -->
                        <div class="overlay-content">
                            <div class="text-center">
                                <h5>Screaming Females</h5>
                                <p>January 15, 2018</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
